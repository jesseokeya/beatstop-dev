
<section class="hbox stretch">
                <aside class="aside-lg bg-light lter b-r">
                  <section class="vbox">
                    <section class="scrollable">
                      <div class="wrapper" style="background: url('<%=@artist.cover_photo.expiring_url(500)%>');">
                        <div class="text-center m-b m-t">
                          <a href="#" class="thumb-lg">
                            <img src="<%=@artist.avatar.expiring_url(500)%>" class="img-circle">
                          </a>
                          <div>
                            <div class="h3 m-t-xs m-b-xs make-white"><%="#{@artist.first_name} #{@artist.last_name}"%></div>
                            <%=link_to "Edit your profile",edit_artist_path(@artist), class:"btn btn-rounded btn-success" %>
<!--                             <small class="text-muted"><i class="fa fa-map-marker"></i> London, UK</small>
 -->                          </div>                
                        </div>
                        <div class="panel wrapper">
                          <div class="row text-center">
                            <div class="col-xs-6">
                              <a href="#">
                                <span class="m-b-xs h4 block"><%=@artist.followers.count || 0 %></span>
                                <small class="text-muted">Followers</small>
                              </a>
                            </div>
                            <div class="col-xs-6">
                              <a href="#">
                                <span class="m-b-xs h4 block"><%=@artist.following.count || 0 %></span>
                                <small class="text-muted">Following</small>
                              </a>
                            </div>
                          </div>
                        </div>
                        <% if current_user.artist != @artist %>
                        <div class="btn-group btn-group-justified m-b">
                          <a class="btn btn-success btn-rounded" data-toggle="button">
                            <span class="text">
                              <i class="fa fa-eye"></i> Follow
                            </span>
                            <span class="text-active">
                              <i class="fa fa-eye"></i> Following
                            </span>
                          </a>
                          <a class="btn btn-dark btn-rounded">
                            <i class="fa fa-comment-o"></i> Chat
                          </a>
                        </div>
                        <% end %>
                        <div class="make-white">
                          <small class="text-uc text-lg make-white">About Me</small>
                          <p><%=@artist.bio%></p>
                          <div class="line"></div>
                          <% if @artist.fb_link.to_s!="" ||@artist.twitter.to_s!="" ||@artist.soundclound.to_s!="" %>
                          <small class="text-uc text-xs text-muted">Connections</small>
                          <% end %>
                          <p class="m-t-sm">
                          <%if @artist.fb_link.to_s!=""%>
                            <a href="<%=@artist.fb_link%>" class="btn btn-rounded btn-facebook btn-icon"><i class="fa fa-facebook"></i></a>
                            <% end %>
                            <% if @artist.twitter.to_s!=""%>
                            <a href="<%=@artist.twitter%>" class="btn btn-rounded btn-twitter btn-icon"><i class="fa fa-twitter"></i></a>
                            <% end %>
                            <% if @artist.soundclound.to_s!=""%> 
                            <a href="<%=@artist.soundclound%>" class="btn btn-rounded btn-gplus btn-icon"><i class="fa fa-soundcloud"></i></a>
                            <% end %>
                          </p>
                        </div>
                      </div>
                    </section>
                  </section>
