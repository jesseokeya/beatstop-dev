@font-face{
	font-family:'Poppins';
	src:url('<%=image_path "400.eot"%>');
	src:url('<%=image_path "400.eot?#iefix"%>') format('embedded-opentype'),
	url('<%=image_path "400.woff2"%>') format('woff2'),
	url('<%=image_path "400.woff"%>') format('woff'),
	url('<%=image_path "400.ttf"%>') format('truetype'),
	url('<%=image_path "400.svg#embed-poppins"%>') format('svg');
	font-weight: 400;
	font-style: normal;
}

